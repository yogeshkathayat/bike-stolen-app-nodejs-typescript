    "lusca": "^1.5.2",
    "method-override": "^2.3.8",
    "morgan": "^1.9.1",
    "node-cron": "^2.0.3",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "sequelize": "^4.31.2",
    "sequelize-cli": "^3.2.0",
    "sequelize-typescript": "^0.6.8",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.2.1"
